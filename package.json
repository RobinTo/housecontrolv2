{
  "name": "housecontrol",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Robin Tollisen",
  "license": "none",
  "scripts": {
    "start": "npm run server",
    "build": "rimraf build && npm run buildClient && npm run buildServer",
    "server": "cross-env NODE_ENV=production node build/prodserverbundle.js",
    "dev": "rimraf build && npm run buildDevServer && concurrently --kill-others \"npm run devserver\" \"npm run webpackdev\"",
    "webpackdev": "cross-env NODE_ENV=development webpack-dev-server",
    "devserver": "cross-env NODE_ENV=development node build/devserverbundle.js",
    "buildClient": "cross-env NODE_ENV=production webpack -p --config webpack.prod.config.js",
    "buildDevServer": "cross-env NODE_ENV=development webpack --config webpack.devserver.config.js",
    "buildServer": "cross-env NODE_ENV=production webpack -p --config webpack.server.config.js"
  },
  "dependencies": {
    "cross-env": "^3.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "node-noop": "^1.0.0",
    "node-sass": "^4.5.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  }
}
